<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="45000000" id="com.bookman.ggwallet" ios-CFBundleVersion="0.4.0" version="0.4.0">
    <name>GgWallet</name>
    <description>
      GameGold Bitcoin Wallet
    </description>
    <author email="support@bookman.com" href="https://bookman.com">
        bookman Inc.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <preference name="iosPersistentFileLocation" value="Library" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="Orientation" value="default" />
    <preference name="cordova-custom-config-stoponerror" value="true" />
    <preference name="StatusBarBackgroundColor" value="#192c3a" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value="200" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="loadUrlTimeoutValue" value="60000" />
    <preference name="android-minSdkVersion" value="22" />
    <preference name="deployment-target" value="10.3" />
    <plugin name="cordova-clipboard" spec="1.1.1" />
    <plugin name="cordova-plugin-android-fingerprint-auth" spec="1.4.2" />
    <plugin name="cordova-plugin-device" spec="2.0.1" />
    <plugin name="cordova-plugin-globalization" spec="1.0.9" />
    <plugin name="cordova-plugin-qrscanner" spec="2.5.0" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="2.0.2" />
    <plugin name="cordova-plugin-touch-id" spec="3.3.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <plugin name="cordova-plugin-x-socialsharing" spec="5.3.2" />
    <plugin name="cordova-plugin-x-toast" spec="2.6.0" />
    <plugin name="ionic-plugin-keyboard" spec="2.2.1" />
    <plugin name="cordova-plugin-screen-orientation" spec="3.0.1" />
    <plugin name="cordova-plugin-spinner-dialog" spec="1.3.1" />
    <plugin name="cordova-plugin-dialogs" spec="2.0.1" />
    <plugin name="cordova-plugin-network-information" spec="2.0.1" />
    <plugin name="cordova-plugin-uniquedeviceid" spec="1.3.2" />
    <plugin name="cordova-plugin-vibration" spec="3.1.0" />
    <plugin name="cordova-plugin-file" spec="6.0.1" />
    <plugin name="cordova-custom-config" spec="5.0.2" />
    <plugin name="cordova-plugin-customurlscheme" spec="https://github.com/cmgustavo/Custom-URL-scheme.git#v4.0">
        <variable name="URL_SCHEME" value="bitcoin" />
        <variable name="SECOND_URL_SCHEME" value="ggwallet" />
    </plugin>
    <plugin name="cordova-plugin-ionic-webview" spec="1.1.16" />
    <platform name="ios">
        <config-file parent="UIStatusBarHidden" platform="ios" target="*-Info.plist">
            <true />
        </config-file>
        <config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios" target="*-Info.plist">
            <false />
        </config-file>
        <config-file parent="ITSAppUsesNonExemptEncryption" target="*-Info.plist">
            <false />
        </config-file>
        <config-file parent="LSApplicationQueriesSchemes" platform="ios" target="*-Info.plist">
            <array>
                <string>whatsapp</string>
                <string>fb</string>
                <string>twitter</string>
            </array>
        </config-file>
        <icon height="180" src="resources/ggwallet/ios/icon/icon-60@3x.png" width="180" />
        <icon height="60" src="resources/ggwallet/ios/icon/icon-60.png" width="60" />
        <icon height="120" src="resources/ggwallet/ios/icon/icon-60@2x.png" width="120" />
        <icon height="76" src="resources/ggwallet/ios/icon/icon-76.png" width="76" />
        <icon height="152" src="resources/ggwallet/ios/icon/icon-76@2x.png" width="152" />
        <icon height="167" src="resources/ggwallet/ios/icon/icon-83.5@2x.png" width="167" />
        <icon height="40" src="resources/ggwallet/ios/icon/icon-40.png" width="40" />
        <icon height="80" src="resources/ggwallet/ios/icon/icon-40@2x.png" width="80" />
        <icon height="57" src="resources/ggwallet/ios/icon/icon.png" width="57" />
        <icon height="114" src="resources/ggwallet/ios/icon/icon@2x.png" width="114" />
        <icon height="72" src="resources/ggwallet/ios/icon/icon-72.png" width="72" />
        <icon height="144" src="resources/ggwallet/ios/icon/icon-72@2x.png" width="144" />
        <icon height="29" src="resources/ggwallet/ios/icon/icon-small.png" width="29" />
        <icon height="58" src="resources/ggwallet/ios/icon/icon-small@2x.png" width="58" />
        <icon height="87" src="resources/ggwallet/ios/icon/icon-small@3x.png" width="87" />
        <icon height="50" src="resources/ggwallet/ios/icon/icon-50.png" width="50" />
        <icon height="100" src="resources/ggwallet/ios/icon/icon-50@2x.png" width="100" />
        <icon height="1024" src="resources/ggwallet/ios/app-store/icon-1024.png" width="1024" />
        <splash src="resources/ggwallet/ios/splash/Default@2x~universal~anyany.png" />
        <splash src="resources/ggwallet/ios/splash/Default@3x~universal~anyany.png" />
        <allow-navigation href="*" />
    </platform>
    <platform name="android">
        <preference name="android-manifest/application/@android:allowBackup" value="false" />
        <icon density="ldpi" src="resources/ggwallet/android/icon/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="resources/ggwallet/android/icon/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="resources/ggwallet/android/icon/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="resources/ggwallet/android/icon/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="resources/ggwallet/android/icon/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="resources/ggwallet/android/icon/drawable-xxxhdpi-icon.png" />
        <splash density="land-hdpi" src="resources/ggwallet/android/splash/drawable-land-hdpi-screen.png" />
        <splash density="land-ldpi" src="resources/ggwallet/android/splash/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="resources/ggwallet/android/splash/drawable-land-mdpi-screen.png" />
        <splash density="land-xhdpi" src="resources/ggwallet/android/splash/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="resources/ggwallet/android/splash/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="resources/ggwallet/android/splash/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-hdpi" src="resources/ggwallet/android/splash/drawable-port-hdpi-screen.png" />
        <splash density="port-ldpi" src="resources/ggwallet/android/splash/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="resources/ggwallet/android/splash/drawable-port-mdpi-screen.png" />
        <splash density="port-xhdpi" src="resources/ggwallet/android/splash/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="resources/ggwallet/android/splash/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="resources/ggwallet/android/splash/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <engine name="android" spec="7.0.0" />
    <engine name="ios" spec="4.5.4" />
</widget>
